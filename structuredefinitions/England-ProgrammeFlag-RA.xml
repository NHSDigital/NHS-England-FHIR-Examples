<StructureDefinition xmlns="http://hl7.org/fhir">
    <id value="ProgrammeFlag" />
    <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-category">
        <valueString value="Base.Management" />
    </extension>
    <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-security-category">
        <valueCode value="patient" />
    </extension>
    <url value="https://fhir.nhs.uk/England/StructureDefinition/ProgrammeFlag" />
    <version value="0.1.0" />
    <name value="ProgrammeFlag" />
    <title value="NHSE Programme Flag Profile" />
    <status value="draft" />
    <date value="2024-02-14" />
    <description value="Record an individual flag for an NHS England programme." />
    <fhirVersion value="4.0.1" />
    <mapping>
        <identity value="rim" />
        <uri value="http://hl7.org/v3" />
        <name value="RIM Mapping" />
    </mapping>
    <mapping>
        <identity value="w5" />
        <uri value="http://hl7.org/fhir/fivews" />
        <name value="FiveWs Pattern Mapping" />
    </mapping>
    <kind value="resource" />
    <abstract value="false" />
    <type value="Flag" />
    <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Flag" />
    <derivation value="constraint" />
    <differential>
        <element id="Flag.extension">
            <path value="Flag.extension" />
            <slicing>
                <discriminator>
                    <type value="value" />
                    <path value="url" />
                </discriminator>
                <ordered value="false" />
                <rules value="open" />
            </slicing>
        </element>
        <element id="Flag.category">
            <path value="Flag.category" />
            <slicing>
                <discriminator>
                    <type value="pattern" />
                    <path value="$this" />
                </discriminator>
                <rules value="open" />
            </slicing>
            <min value="2" />
        </element>
        <element id="Flag.category:patientFlag">
            <path value="Flag.category" />
            <sliceName value="patientFlag" />
            <min value="1" />
            <max value="*" />
            <mustSupport value="true" />
        </element>
        <element id="Flag.category:programmeFlag">
            <path value="Flag.category" />
            <sliceName value="programmeFlag" />
            <min value="1" />
            <max value="*" />
            <mustSupport value="true" />
        </element>
        <element id="Flag.code">
            <path value="Flag.code" />
            <mustSupport value="true" />
        </element>
    </differential>
</StructureDefinition>
