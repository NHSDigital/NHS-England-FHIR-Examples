<?xml version="1.0" encoding="UTF-8" ?>
 <root>
     <resourceType>StructureDefinition</resourceType>
     <id>ProgrammeFlag</id>
     <url>https://fhir.nhs.uk/England/StructureDefinition/ProgrammeFlag</url>
     <version>0.1.0</version>
     <name>ProgrammeFlag</name>
     <title>NHSE Programme Flag Profile</title>
     <status>draft</status>
     <date>2024-02-14</date>
     <description>Record an individual flag for an NHS England programme.</description>
     <fhirVersion>4.0.1</fhirVersion>
     <kind>resource</kind>
     <abstract>false</abstract>
     <type>Flag</type>
     <baseDefinition>http://hl7.org/fhir/StructureDefinition/Flag</baseDefinition>
     <derivation>constraint</derivation>
     <differential>
         <element>
             <id>Flag.extension</id>
             <path>Flag.extension</path>
             <slicing>
                 <discriminator>
                     <type>value</type>
                     <path>url</path>
                 </discriminator>
                 <ordered>false</ordered>
                 <rules>open</rules>
             </slicing>
         </element>
         <element>
             <id>Flag.category</id>
             <path>Flag.category</path>
             <slicing>
                 <discriminator>
                     <type>pattern</type>
                     <path>$this</path>
                 </discriminator>
                 <rules>open</rules>
             </slicing>
             <min>2</min>
         </element>
         <element>
             <id>Flag.category:patientFlag</id>
             <path>Flag.category</path>
             <sliceName>patientFlag</sliceName>
             <min>1</min>
             <max>*</max>
             <mustSupport>true</mustSupport>
         </element>
         <element>
             <id>Flag.category:programmeFlag</id>
             <path>Flag.category</path>
             <sliceName>programmeFlag</sliceName>
             <min>1</min>
             <max>*</max>
             <mustSupport>true</mustSupport>
         </element>
         <element>
             <id>Flag.code</id>
             <path>Flag.code</path>
             <mustSupport>true</mustSupport>
         </element>
     </differential>
 </root>
