<?xml version="1.0" encoding="UTF-8" ?>
 <root>
     <resourceType>StructureDefinition</resourceType>
     <id>PatientFlag</id>
     <url>https://fhir.nhs.uk/England/StructureDefinition/PatientFlag</url>
     <version>0.1.0</version>
     <name>PatientFlag</name>
     <title>NHSE Patient Flag Profile</title>
     <status>draft</status>
     <date>2024-02-14</date>
     <description>Record whether a Patient Flags exists</description>
     <fhirVersion>4.0.1</fhirVersion>
     <kind>resource</kind>
     <abstract>false</abstract>
     <type>Flag</type>
     <baseDefinition>http://hl7.org/fhir/StructureDefinition/Flag</baseDefinition>
     <derivation>constraint</derivation>
     <differential>
         <element>
             <id>Flag.contained</id>
             <path>Flag.contained</path>
             <slicing>
                 <discriminator>
                     <type>profile</type>
                     <path>contained</path>
                 </discriminator>
                 <rules>open</rules>
             </slicing>
             <min>1</min>
         </element>
         <element>
             <id>Flag.contained:provenance</id>
             <path>Flag.contained</path>
             <sliceName>provenance</sliceName>
             <min>1</min>
             <max>1</max>
         </element>
         <element>
             <id>Flag.extension</id>
             <path>Flag.extension</path>
             <slicing>
                 <discriminator>
                     <type>value</type>
                     <path>url</path>
                 </discriminator>
                 <ordered>false</ordered>
                 <rules>open</rules>
             </slicing>
         </element>
         <element>
             <id>Flag.code</id>
             <path>Flag.code</path>
             <mustSupport>true</mustSupport>
         </element>
     </differential>
 </root>
