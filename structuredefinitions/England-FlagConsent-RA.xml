<?xml version="1.0" encoding="UTF-8" ?>
 <root>
     <resourceType>StructureDefinition</resourceType>
     <id>FlagConsent</id>
     <url>https://fhir.nhs.uk/England/StructureDefinition/FlagConsent</url>
     <version>0.1.0</version>
     <name>FlagConsent</name>
     <title>NHSE Flag Consent Profile</title>
     <status>draft</status>
     <date>2024-02-14</date>
     <description>Record consent to capture information for NHS England flags</description>
     <fhirVersion>4.0.1</fhirVersion>
     <kind>resource</kind>
     <abstract>false</abstract>
     <type>Consent</type>
     <baseDefinition>http://hl7.org/fhir/StructureDefinition/Consent</baseDefinition>
     <derivation>constraint</derivation>
     <differential>
         <element>
             <id>Consent.contained</id>
             <path>Consent.contained</path>
             <slicing>
                 <discriminator>
                     <type>profile</type>
                     <path>contained</path>
                 </discriminator>
                 <rules>open</rules>
             </slicing>
             <min>1</min>
         </element>
         <element>
             <id>Consent.contained:provenance</id>
             <path>Consent.contained</path>
             <sliceName>provenance</sliceName>
             <min>1</min>
             <max>1</max>
         </element>
         <element>
             <id>Consent.extension</id>
             <path>Consent.extension</path>
             <slicing>
                 <discriminator>
                     <type>value</type>
                     <path>url</path>
                 </discriminator>
                 <ordered>false</ordered>
                 <rules>open</rules>
             </slicing>
         </element>
         <element>
             <id>Consent.performer</id>
             <path>Consent.performer</path>
             <max>1</max>
             <type>
                 <code>Reference</code>
                 <targetProfile>http://hl7.org/fhir/StructureDefinition/Organization</targetProfile>
                 <targetProfile>http://hl7.org/fhir/StructureDefinition/Patient</targetProfile>
                 <targetProfile>http://hl7.org/fhir/StructureDefinition/Practitioner</targetProfile>
             </type>
         </element>
         <element>
             <id>Consent.policy</id>
             <path>Consent.policy</path>
             <min>1</min>
             <max>1</max>
         </element>
         <element>
             <id>Consent.policy.uri</id>
             <path>Consent.policy.uri</path>
             <min>1</min>
         </element>
         <element>
             <id>Consent.policyRule</id>
             <path>Consent.policyRule</path>
             <min>1</min>
         </element>
         <element>
             <id>Consent.provision.purpose</id>
             <path>Consent.provision.purpose</path>
             <min>1</min>
             <max>1</max>
         </element>
     </differential>
 </root>
